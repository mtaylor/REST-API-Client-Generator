# -*- encoding: utf-8 -*-
lib = File.expand_path('../lib', __FILE__)
$LOAD_PATH.unshift(lib) unless $LOAD_PATH.include?(lib)
Gem::Specification.new do |gem|
  gem.name          = '<%= @gemspec[:name] %>'
  gem.version       = '<%= @gemspec[:version] %>'
  gem.authors       = ['Autogenerated From ReACG']
  gem.email         = ['autogen@reacg.org']
  gem.description   = 'autogenerated ReST Client lib'
  gem.summary       = 'autogenerated ReSt Client lib'
  gem.homepage      = 'http://www.reacg.org'
  gem.files = Dir['{lib}/**/*']
  gem.require_paths = ['lib']
  <% @gemspec[:dependencies].each do | d | %>
  gem.add_dependency '<%= d[:name] %>', '<%= d[:version] %>'
  <% end %>
end